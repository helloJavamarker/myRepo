<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mark.zhang.mapper.ImportTestMapper">

    <insert id="importFirst">
        insert into t_first (`name`, `age`,`height`) values (#{importTestBean.name}, #{importTestBean.age},#{importTestBean.height});
    </insert>

    <insert id="importSecond">
        insert into t_second (`hobby`, `money`,`play`) values (#{importTestBean.hobby}, #{importTestBean.money},#{importTestBean.money});
    </insert>
</mapper>